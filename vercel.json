{
  "builds": [
    {
      "src": "api/blog.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/blog/[id].js",
      "use": "@vercel/node"
    }
  ],
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": "public/**"
    }
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/blog/[id]",
      "dest": "/api/blog/[id].js"
    },
    {
      "src": "/api/blog",
      "dest": "/api/blog.js"
    }
  ]
}
